<h1>Chitter</h1>

<form action="/post" method="post">
  <input type="text" id="peep_content" name="peep_content">
  <input type="submit" value="Post">
</form>

<ol class="peeps" style="list-style-type:none">
  <% @peeps.reverse.each do |peep| %>
    <div class="peep">
      <%= peep.message %>
      <%= peep.time %>
    </div>
  <% end %>
</ol>
